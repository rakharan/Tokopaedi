{
  "name": "tokopaedi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -e ts --exec \"ts-node -r tsconfig-paths/register -r ./src/app.ts\"",
    "prod": "cross-env NODE_ENV=production nodemon -e --exec node ./build/app.js",
    "dev:docker": "cross-env NODE_ENV=dockerdev nodemon -L -e ts --exec \"ts-node -r tsconfig-paths/register -r ./src/app.ts\"",
    "migrate:run": "cross-env NODE_ENV=development npx typeorm-ts-node-commonjs migration:run -d src/infrastructure/mysql/connection.ts",
    "migrate:create": "npx typeorm-ts-node-commonjs migration:create",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && ts-node copy-templates.ts",
    "test": "cross-env NODE_ENV=testing vitest",
    "test:ui": "cross-env NODE_ENV=testing vitest --ui",
    "test:coverage": "cross-env NODE_ENV=testing vitest --coverage",
    "generate:proto": "grpc_tools_node_protoc --plugin=protoc-gen-ts=\"%CD%\\node_modules\\.bin\\protoc-gen-ts.cmd\" --ts_out=client_grpc1,server_grpc1:./src/generated_grpc --proto_path=./src/proto ./src/proto/*.proto"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "7.4.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/swagger": "^8.7.0",
    "@fastify/swagger-ui": "^1.9.2",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@protobuf-ts/plugin": "^2.11.0",
    "@protobuf-ts/runtime": "^2.11.0",
    "@protobuf-ts/runtime-rpc": "^2.11.0",
    "@types/google-protobuf": "^3.15.12",
    "ajv": "^8.12.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.0",
    "cloudinary": "^2.0.0-rc",
    "dotenv-flow": "^4.1.0",
    "fastify": "4.24.3",
    "fastify-healthcheck": "^4.4.0",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "4.5.1",
    "format-unicorn": "^1.1.1",
    "google-protobuf": "^3.21.4",
    "grpc-tools": "^1.13.0",
    "handlebars": "^4.7.8",
    "indonesian-profanity": "^1.0.1",
    "ioredis": "^5.3.2",
    "joi": "17.11.0",
    "jsonwebtoken": "9.0.2",
    "key-pagination-sql": "^1.0.2",
    "moment": "2.29.4",
    "moment-timezone": "^0.5.43",
    "mysql2": "^3.9.8",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.9",
    "pino-pretty": "^10.2.3",
    "typeorm": "0.3.17",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "devDependencies": {
    "@types/autocannon": "^7.12.5",
    "@types/bcrypt": "5.0.2",
    "@types/format-unicorn": "^1.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.1.3",
    "autocannon": "^7.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.3.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.2",
    "vitest": "^1.1.3"
  },
